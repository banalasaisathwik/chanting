{
  "expo": {
    "name": "my-expo-app",
    "slug": "my-expo-app",
    "version": "1.0.0",
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": [
      [
        "@react-native-google-signin/google-signin"
      ],
      "expo-audio",
      "expo-secure-store",
      "expo-dev-client",
      "expo-notifications"
    ],
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": [
          "audio"
        ],
        "NSMicrophoneUsageDescription": "This app needs access to audio for volume detection."
      }
    },
    "android": {
    "googleServicesFile": "./android/app/google-services.json",

      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS",
        "android.permission.ACCESS_NOTIFICATION_POLICY"
      ],
      "package": "com.sai_sathwik.myexpoapp"
    },
    "extra": {
      "eas": {
        "projectId": "e0b80148-dcec-4c3b-a1da-2cf82f401447"
      }
    },
    "owner": "sai_sathwik"
  }
}
